namespace app\admin\controller;

use think\Controller;

class Admintype extends Controller
{
    public function admintype(){
        $aid=$_POST["aid"];
        $astatus=$_POST["astatus"];
        db('admin')->where('aid',$aid)->setField('astatus',$astatus);
    }
}